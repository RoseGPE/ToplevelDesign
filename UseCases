rectangle Racecar {
    rectangle "Driver Interface" {
        Driver <-> [Steering]
        Driver -> (Accelerate)
        Driver -> (Brake)
        Driver -> (Shit)
        Driver -> (Clutch)
        Driver <- (Driver Restrainment)
    }

}


